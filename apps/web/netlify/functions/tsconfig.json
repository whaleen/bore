{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../.netlify/functions",
    "rootDir": ".",
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
