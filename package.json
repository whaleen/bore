{
  "private": true,
  "name": "bore",
  "version": "1.0.0",
  "description": "Web3-enabled proxy/VPN node management platform with Solana authentication, Chrome extension, and Vultr cloud integration for decentralized network infrastructure",
  "keywords": [
    "proxy",
    "vpn",
    "node-management",
    "solana",
    "web3",
    "blockchain",
    "chrome-extension",
    "netlify",
    "postgresql",
    "prisma",
    "vultr",
    "cloud-infrastructure",
    "react",
    "typescript",
    "monorepo",
    "turbo",
    "vite",
    "tailwindcss",
    "daisyui"
  ],
  "author": "whaleen",
  "license": "MIT",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "prisma:generate": "npx prisma generate --schema apps/web/prisma/schema.prisma",
    "dev:web": "npm run dev --workspace=@bore/web",
    "build:web": "npm run prisma:generate && turbo run build --filter @bore/web",
    "lint:web": "turbo run lint --filter @bore/web",
    "dev:chrome-extension": "npm run dev --workspace=@bore/chrome-extension",
    "build:chrome-extension": "turbo run build --filter @bore/chrome-extension",
    "lint:chrome-extension": "turbo run lint --filter @bore/chrome-extension",
    "build": "turbo run build",
    "dev": "turbo run dev"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@prisma/client": "^5.22.0",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.5",
    "autoprefixer": "^10.4.20",
    "countries-and-timezones": "^3.7.2",
    "country-flag-icons": "^1.5.13",
    "country-list": "^2.3.0",
    "country-region-data": "^3.1.0",
    "daisyui": "^4.12.14",
    "lucide-react": "^0.462.0",
    "postcss": "^8.4.49",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^7.0.1",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@eslint/js": "^9.15.0",
    "@storybook/addon-themes": "^8.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "express": "^4.21.1",
    "globals": "^15.12.0",
    "netlify-cli": "^17.37.2",
    "rollup-plugin-copy": "^3.5.0",
    "tsoa": "^6.5.1",
    "turbo": "latest",
    "typescript": "~5.1.6",
    "vite": "^6.0.0"
  }
}
